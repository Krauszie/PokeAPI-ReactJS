{
  "name": "vite-api-02",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build-css": "npx tailwindcss build -i ./src/index.css -o ./src/output.css",
    "watch-css": "npx tailwindcss build -i ./src/index.css -o ./src/output.css -w",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev": "concurrently \"npm run watch-css\" \"npm run start\""
  },
  "dependencies": {
    "axios": "^1.7.2",
    "concurrently": "^8.2.2",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "tailwindcss": "^3.4.4",
    "vite": "^5.2.0"
  }
}
